! Xresources file

!!! Fake transparency settings
!URxvt*transparent: true
!! URxvt*shading: 0 to 99 darkens, 101 to 200 lightens
!URxvt*shading: 35
!! Background color
!URxvt*background: black

!! True transparency settings
URxvt*depth: 32
URxvt*background: [65]#000000

! Font color
URxvt*foreground: white

! Fonts
URxvt*font:       xft:DejaVu Sans Mono for Powerline:size=12:antialias=true
URxvt*boldFont:   xft:DejaVu Sans Mono for Powerline:bold:size=12:antialias=true
URxvt*italicFont: xft:DejaVu Sans Mono for Powerline:italic:size=12:antialias=true
URxvt*letterSpace: -1

! Other colors
URxvt*color0:  #2e3436
URxvt*color1:  #a40000
URxvt*color2:  #4e9a06
URxvt*color3:  #c4a000
URxvt*color4:  #3465a4
URxvt*color5:  #75507b
URxvt*color6:  #ce5c00
URxvt*color7:  #babdb9
URxvt*color8:  #555753
URxvt*color9:  #ef2929
URxvt*color10: #8ae234
URxvt*color11: #fce94f
URxvt*color12: #729fcf
URxvt*color13: #ad7fa8
URxvt*color14: #fcaf3e
URxvt*color15: #eeeeec

! scrollbar style - rxvt (default), plain (most compact), next, or xterm
URxvt*scrollstyle: plain
